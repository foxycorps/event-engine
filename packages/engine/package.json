{
    "name": "@foxycorps/event-engine",
    "version": "1.0.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/*"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "inputs": [
                "./dist/inputs.d.ts"
            ],
            "outputs": [
                "./dist/output.d.ts"
            ]
        }
    },
    "exports": {
        ".": "./dist/index.js",
        "./inputs": {
            "import": "./dist/inputs.js",
            "types": "./dist/inputs.d.ts"
        },
        "./outputs": {
            "import": "./dist/outputs.js",
            "types": "./dist/outputs.d.ts"
        }
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": ""
    },
    "dependencies": {
        "@foxycorps/nanoevent": "^4.1.0"
    },
    "devDependencies": {
        "@types/react": "*",
        "tsup": "*",
        "vitest": "*"
    },
    "peerDependencies": {
        "axios": "*",
        "react": "*"
    }
}